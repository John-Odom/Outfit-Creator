
<%=form_for @user do |f|%>
    <%=f.label 'First Name: '%>
    <%=f.text_field :first_name%>
    <%=f.label 'Last Name: '%>
    <%=f.text_field :last_name%>
    <%=f.label 'Age: '%>
    <%=f.number_field :age%>
    <%=f.label 'Bio: '%>
    <%=f.text_area :bio%>
    <%=f.label 'Username: '%>
    <%=f.text_field :username%>
    <%=f.label 'Password: '%>
    <%=f.password_field :password%>
    <%=f.submit%>
<%end%>
